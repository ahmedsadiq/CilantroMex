<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/RelativeLayout" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="#000000">

	<RelativeLayout android:id="@+id/header"
		android:layout_width="fill_parent" android:gravity="center_vertical"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:layout_alignParentTop="true" android:background="@drawable/topbar_logo">

	</RelativeLayout>

	<WebView android:id="@+id/webview" android:layout_width="match_parent"
		android:layout_height="fill_parent" android:scrollbars="none"
		android:layout_below="@+id/header" />



</RelativeLayout>